{
  "name": "cibmtr-ehrapp",
  "version": "2.6.0-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run clean && npm run build:watch\" \"npm run serve\"",
    "start:mock": "ng serve --configuration=mock",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:dev": "ng build --configuration=dev",
    "build:qa": "ng build --configuration=qa",
    "build:preprod": "ng build --configuration=preprod",
    "build:docker": "docker build -t web-cibmtr-reporting .",
    "run:docker": "docker run -d -p 80:80 --name web-cibmtr-reporting web-cibmtr-reporting",
    "test": "ng test",
    "test:ci": "ng test",
    "lint:ci": "npm run lint -- -f node_modules/eslint-bamboo-formatter/reporter.js",
    "tslint:ci": "npm run --silent tslint -- --force=true --format tslint-junit-formatter > tslint-junit.xml",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^10.2.5",
    "@angular/common": "^10.2.5",
    "@angular/compiler": "^10.2.5",
    "@angular/core": "^10.2.5",
    "@angular/forms": "^10.2.5",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^10.2.5",
    "@angular/platform-browser-dynamic": "^10.2.5",
    "@angular/router": "^10.2.5",
    "@nmdp/nmdp-login": "1.2.0",
    "@okta/okta-signin-widget": "^4.5.2",
    "angular-2-local-storage": "^3.0.2",
    "bootstrap": "^3.4.1",
    "core-js": "^2.6.11",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ngx-bootstrap": "^5.2.0",
    "node-sass": "^4.13.0",
    "rxjs": "6.6.7",
    "rxjs-compat": "6.3.3",
    "uuid": "^3.4.0",
    "zone.js": "~0.11.4",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.3",
    "@angular-devkit/build-ng-packagr": "^0.1002.3",
    "@angular/cli": "^10.2.3",
    "@angular/compiler-cli": "^10.2.5",
    "@angular/language-service": "^10.2.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^8.10.59",
    "codelyzer": "^6.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~4.0.7"
  }
}
